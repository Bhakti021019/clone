generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UdyamSubmission {
  id        Int     @id @default(autoincrement())
  aadhaar   String
  pan       String
  pincode   String
  city      String
  state     String
  version   Int     @default(0)
}
